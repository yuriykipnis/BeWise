{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gold-mountain-spa": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/sass"
              ]
            },
            "assets": [
              "src/assets"
            ],
            "styles": [
              "src/sass/styles.scss",
              "src/sass/datatable.scss",
              "src/sass/alert-message.scss",
              "src/sass/slider.scss",
              "src/sass/settings.scss",
              "src/sass/form.scss",
              "src/sass/form-validations.scss",
              "src/sass/progressbar.scss",
              "src/sass/search-bar.scss",
              "src/sass/panel.scss",
              "src/sass/panel-search.scss",
              "src/sass/drag-n-drop.scss",
              "src/sass/dragula.scss",
              "src/sass/tags.scss",
              "src/sass/radio-buttons.scss",
              "src/sass/dropdown.scss",
              "src/sass/animated-navbar.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeng/resources/themes/omega/theme.css"
            ],
            "scripts": [
              "node_modules/chart.js/dist/Chart.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gold-mountain-spa:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gold-mountain-spa:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gold-mountain-spa:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/chart.js/dist/Chart.js"
            ],
            "styles": [
              "src/sass/styles.scss",
              "src/sass/datatable.scss",
              "src/sass/alert-message.scss",
              "src/sass/slider.scss",
              "src/sass/settings.scss",
              "src/sass/form.scss",
              "src/sass/form-validations.scss",
              "src/sass/progressbar.scss",
              "src/sass/search-bar.scss",
              "src/sass/panel.scss",
              "src/sass/panel-search.scss",
              "src/sass/drag-n-drop.scss",
              "src/sass/dragula.scss",
              "src/sass/tags.scss",
              "src/sass/radio-buttons.scss",
              "src/sass/dropdown.scss",
              "src/sass/animated-navbar.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeng/resources/themes/omega/theme.css"
            ],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "gold-mountain-spa-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "gold-mountain-spa:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "gold-mountain-spa",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}